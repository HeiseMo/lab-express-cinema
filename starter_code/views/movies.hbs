<div class="moviesMainBox">
    <div class="mainBoxHeader">
        <p class="title">Ironhack Cinema</p>
        <p class="subtitle">Click on the movie to check the showtimes!</p>
    </div>
    <div class="moviesBox">
        {{#each movieList}}
            <div class="movie">
                <img src="{{image}}" alt="{{title}}">
                <h2>{{title}}</h2>
                <a href="/movies/{{_id}}">See more</a>
            </div>
        {{/each}}
    </div>
</div>